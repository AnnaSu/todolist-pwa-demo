{
  "name": "todolist-pwa-demo",
  "version": "1.0.0",
  "description": "PWA To-Do List with Vanilla JS (sw-precache)",
  "main": "sw.js",
  "scripts": {
    "start": "concurrently --kill-others 'http-server' 'json-server --watch db.json'",
    "build": "sw-precache --config=./src/sw-precache-config.js --verbose"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AnnaSu/todolist-pwa-demo.git"
  },
  "keywords": [
    "PWA"
  ],
  "author": "AnnaSu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AnnaSu/todolist-pwa-demo/issues"
  },
  "homepage": "https://github.com/AnnaSu/todolist-pwa-demo/blob/sw-precache/README.md",
  "devDependencies": {
    "concurrently": "^3.1.0",
    "http-server": "^0.9.0",
    "json-server": "^0.9.4",
    "sw-precache": "^4.3.0",
    "webpack": "^1.14.0"
  }
}
